     <script type="text/javascript">  
     var myarr=new Array(); 
      for(var i=1;i<4;i++) 
          {      myarr[i]=new Array();   
             for(var j=1;j<6;j++) 
              {    
                    myarr[i][j]=i*j; 
                    document.write("myarr["+i+"]["+j+"]的值:"+myarr[i][j]+"<br>");    
              }   
              }  
            </script>










